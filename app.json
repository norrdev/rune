{
  "expo": {
    "name": "Runestone Safari",
    "slug": "runestone-safari",
    "scheme": "runestone-safari",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#866c53"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.runestone-safari"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#866c53"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.runestonesafari"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      "@maplibre/maplibre-react-native",
      "expo-router"
    ]
  }
}
