    "expo": "~54.0.32",
    "expo-constants": "^18.0.12",
    "expo-linking": "^8.0.11",
    "expo-router": "~6.0.22",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "idb": "^8.0.3",
    "maplibre-gl": "^5.15.0",
    "mobx": "^6.15.0",
    "mobx-react-lite": "^4.1.1",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "react-native": "0.81.5",
